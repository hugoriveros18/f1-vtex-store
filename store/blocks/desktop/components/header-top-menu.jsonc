            "id": "moneda",
            "type": "custom",
            "itemProps": {
                "type": "internal",
                "href": "#",
                "tagTitle": "moneda",
                "text": "USD US$"
            },
            "iconProps": {
                "id": "arrow-down-icon",
                "size": 10
            },
            "iconPosition": "right",
            "blockClass": [
                "moneda-item",
                "header__top-section-menu"
            ]
        },
        "blocks": [
            "vtex.menu@2.x:submenu#moneda__submenu"
        ]
    },
