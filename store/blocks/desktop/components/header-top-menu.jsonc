{
    ///////////////////////////////////////////////
    "vtex.menu@2.x:menu#header__opciones-cuenta-menu": {
        "title": "Menu de Opciones de Cuenta",
        "children": [
            "menu-item#localizador-envios",
            "menu-item#ayuda",
            "login#desktop",
            "menu-item#idioma",
            "menu-item#moneda"
        ],
        "props": {
            "blockClass": "header__opciones-cuenta-menu"
        }
    },
    ///////////////////////////////////////////////
    "menu-item#localizador-envios": {
        "title": "Opcion Localizador de Envios",
        "props": {
            "id": "localizador-envios",
            "type": "custom",
            "itemProps": {
                "type": "internal",
                "href": "#",
                "tagTitle": "localizador",
                "text": "Localizador de envíos"
            },
            "blockClass": [
                "header__top-section-menu",
                "localizador-item"
            ]
        }
    },
    "menu-item#ayuda": {
        "title": "Opcion Ayuda",
        "props": {
            "id": "Ayuda",
            "type": "custom",
            "itemProps": {
                "type": "internal",
                "href": "/ayuda",
                "tagTitle": "Ayuda",
                "text": "Ayuda"
            },
            "blockClass": [
                "header__top-section-menu",
                "ayuda-item"
            ]
        }
    },
    "login#desktop": {
        "title": "Login",
        "props": {
            "iconLabel": "Mi cuenta",
            "logInButtonBehavior": "link",
            "accountOptionsButtonBehavior": "popover",
            "accountOptionLinks": [
                {
                    "label": "Mi Cuenta",
                    "path": "/account",
                    "cssClass": "account"
                },
                {
                    "label": "Mis Detalles",
                    "path": "/account#/profile",
                    "cssClass": "account"
                },
                {
                    "label": "Mi Historial De Pedidos",
                    "path": "/account#/orders-history",
                    "cssClass": "account"
                },
                {
                    "label": "Mi Libreta De Direcciones",
                    "path": "/account#/addresses",
                    "cssClass": "account"
                },
                {
                    "label": "Metodos De Pago",
                    "path": "/account#/cards",
                    "cssClass": "account"
                }
            ],
            "blockClass": "header__top-section-login"
        }
    },
    "menu-item#idioma": {
        "title": "Menu de Opciones Idiomas",
        "props": {
            "id": "idioma",
            "type": "custom",
            "itemProps": {
                "type": "internal",
                "href": "#",
                "tagTitle": "idioma",
                "text": "Idioma"
            },
            "iconProps": {
                "id": "arrow-down-icon",
                "size": 10
            },
            "iconPosition": "right",
            "blockClass": [
                "idioma-item",
                "header__top-section-menu"
            ]
        },
        "blocks": [
            "vtex.menu@2.x:submenu#idioma__submenu"
        ]
    },
    "menu-item#moneda": {
        "title": "Menu de Opciones Moneda",
