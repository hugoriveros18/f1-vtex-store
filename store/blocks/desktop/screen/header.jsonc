{
  ///////////////////////////////////////////////
  "header-layout.desktop": {
    "title": "Desktop Header",
    "children": [
      "header-row#desktop-tablet__header--section-one",
      "header-row#desktop-tablet__header--section-two",
      "header-row#desktop-tablet__header--section-three",
      "sticky-layout.stack-container#desktop-tablet__header--section-four"
    ]
  },
  ///////////////////////////////////////////////
  "header-row#desktop-tablet__header--section-one": {
    "title": "Header Section One",
    "children": [
      "drawer#mobile__header--menu",
      "flex-layout.row#global__header--modal-registration",
      "header-spacer",
      "vtex.menu@2.x:menu#desktop-tablet__header--menu-one",
      "minicart.v2#desktop-tablet__header--minicart"
    ],
    "props": {
      "blockClass": "desktop-tablet__header--section-one"
    }
  },
  "header-row#desktop-tablet__header--section-two": {
    "title": "Header Section Two",
    "children": [
      "logo#desktop-tablet__header--logo",
      "search-bar#desktop-tablet__header--search-bar",
      "header-spacer",
      "quick-order"
    ],
    "props": {
      "blockClass": "desktop-tablet__header--section-two"
    }
  },
  "header-row#desktop-tablet__header--section-three": {
    "title": "Header Section Three",
    "children": [
      "vtex.menu@2.x:menu#desktop__header--menu-two"
    ],
    "props": {
      "blockClass": "desktop-tablet__header--section-three"
    }
  },
  "sticky-layout.stack-container#desktop-tablet__header--section-four": {
    "title": "Header Section Four",
    "children": [
      "sticky-layout#desktop-tablet__header--promotion-code"
    ]
  },
  ///////////////////////////////////////////////
  "logo#desktop-tablet__header--logo": {
    "title": "F1 Logo",
    "props": {
      "url": "assets/img/header/f1-logo.svg",
      "href": "/",
      "blockClass": "desktop-tablet__header--logo"
    }
  },
  "search-bar#desktop-tablet__header--search-bar": {
    "title": "Search Bar",
    "blocks": [
      "autocomplete-result-list.v2#header__search-bar--autocomplete"
    ],
    "props": {
      "openAutocompleteOnFocus": true,
      "blockClass": "desktop-tablet__header--search-bar"
    }
  },
  "sticky-layout#desktop-tablet__header--promotion-code": {
    "title": "Sticky Promotion Code",
    "props": {
      "position": "TOP",
      "blockClass": "desktop-tablet__header--promotion-code"
    },
    "children": [
      "flex-layout.row#header__promotion-code"
    ]
  },
  ///////////////////////////////////////////////
  "autocomplete-result-list.v2#header__search-bar--autocomplete": {
    "title": "Autocomplete Feature",
    "blocks": [
      "product-summary"
    ],
    "props": {
      "autocompleteWidth": 39.5,
      "hideTitles": true,
      "blockClass": "header__search-bar--autocomplete"
    }
  }
}