    "flex-layout.col#ayuda__section--one": {
        "title": "Ayuda Custom Page Section One",
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "ayuda__section--one"
        },
        "children": [
            "flex-layout.row#ayuda__cuenta",
            "flex-layout.row#ayuda__pedido",
            "flex-layout.row#ayuda__envio",
            "flex-layout.row#ayuda__tallas",
            "flex-layout.row#ayuda__devoluciones",
            "flex-layout.row#ayuda__ofertas",
            "flex-layout.row#ayuda__seguimiento",
            "flex-layout.row#ayuda__seguridad",
            "flex-layout.row#ayuda__terminos",
            "flex-layout.row#ayuda__privacidad",
            "flex-layout.row#ayuda__cookies",
            "flex-layout.row#ayuda__contactenos"
        ]
    },
    "flex-layout.col#ayuda__section--two": {
        "title": "Ayuda Custom Page Section Two",
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "ayuda__section--two"
        },
        "children": [
            "image#ayuda__contacto",
            "rich-text#ayuda__contacto",
            "link#ayuda__contacto"
        ]
    },


