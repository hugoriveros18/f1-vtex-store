{
    "store.custom#ayuda": {
        "blocks": [
            "flex-layout.row#ayuda"
        ]
    },


    ///////////////////////////////////////////////
    "flex-layout.row#ayuda": {
        "title": "Ayuda Custom Page",
        "props": {
            "blockClass": "ayuda"
