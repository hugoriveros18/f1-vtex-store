            "blockClass": [
                "disclosure__trigger--general",
                "details__section"
            ]
        },
        "children": [
            "product-specification-group#details_section"
        ]
    },
    "disclosure-trigger#description__section": {
        "title": "Description Section Trigger",
        "props": {
            "blockClass": [
                "disclosure__trigger--general",
                "description__section"
            ]
        },
        "children": [
            "flex-layout.row#description__section--visible"
        ]
    },
    "disclosure-content#description__section": {
        "title": "Description Section Content",
        "props": {
            "blockClass": [
                "disclosure__trigger--general",
                "description__section"
            ]
        },
        "children": [
            "product-description#product__description"
        ]
