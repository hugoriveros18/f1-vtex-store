        "title": "Product Order Devolutions Icon",
        "props": {
            "id": "return-box-icon",
            "size": 16
        }
    },
    "link#order__devolutions": {
        "title": "Product Order Devolutions Link",
        "props": {
            "blockClass": "order__devolutions",
            "label": "DEVOLUCIONES 90 DÍAS",
            "href": "#"
        }
    },
