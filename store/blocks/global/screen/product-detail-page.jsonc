{
    "store.product": {
        "children": [
            "cart-total-price",
            "flex-layout.row#product_breadcrumb--container",
            "flex-layout.row#product_info--container",
            "flex-layout.row#product_tabs--container",
            "modal-trigger#product__add-to-cart-info"
