    "flex-layout.row#description__section--visible": {
        "title": "Description Section Visible",
        "props": {
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": [
                "simulator__visible",
                "description__section--visible"
            ]
        },
        "children": [
            "rich-text#description__section--title",
            "disclosure-state-indicator#open__close"
        ]
    },
    "product-description#product__description": {
        "title": "Product Description",
        "props": {
            "showTitle": false,
            "blockClass": "product__description"
        }
    },
    ///////////////////////////////////////////////
    "rich-text#delivery__simulator--title": {
        "title": "Delivery Simulator Title",
        "props": {
            "text": "Envíos",
            "blockClass": [
                "product__disclosure--title",
                "delivery__simulator--title"
            ]
        }
    },
    "rich-text#details__section--title": {
        "title": "Details Section Title",
        "props": {
            "text": "Detalles",
            "blockClass": [
                "product__disclosure--title",
                "details__section--title"
            ]
        }
    },
    "rich-text#description__section--title": {
        "title": "Description Section Title",
        "props": {
            "text": "Descripción",
            "blockClass": [
                "product__disclosure--title",
                "description__section--title"
            ]
        }
    },
    "disclosure-state-indicator#open__close": {
        "props": {
            "Show": "icon#angle__down",
            "Hide": "icon#angle__up"
        }
    },
    "product-specification#details__section--product": {
        "children": [
            "product-specification-text#details__section--product-title",
            "product-specification-values#details__section--product-value"
        ]
    },
    ///////////////////////////////////////////////
    "icon#angle__down": {
        "props": {
            "id": "nav-plus",
            "size": 12
        }
    },
    "icon#angle__up": {
        "props": {
            "id": "nav-minus",
            "size": 12
        }
    },
    "product-specification-text#details__section--product-title": {
        "title": "Product Specifications Title",
        "props": {
            "message": "{specificationName}: ",
            "blockClass": "details__section--product-title"
        }
    },
    "product-specification-values#details__section--product-value": {
        "title": "Product Specifications Values",
        "props": {
            "blockClass": "details__section--product-value"
        },
        "children": [
            "product-specification-text#section__product--value-text"
        ]
    },
    ///////////////////////////////////////////////
    "product-specification-text#section__product--value-text": {
        "title": "Product Specifications Values Text",
        "props": {
            "message": "{specificationValue}"
        }
    }
