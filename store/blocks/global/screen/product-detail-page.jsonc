        "children": [
            "flex-layout.col#order__quantity--layout",
            "flex-layout.col#order__add--button-layout"
        ]
    },
    "flex-layout.row#page__order--devolutions": {
        "title": "Product Devolutions Container",
        "props": {
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": "page__order--devolutions"
        },
        "children": [
            "icon#order__devolutions",
            "link#order__devolutions"
        ]
    },
    "flex-layout.col#dropdown__delivery--layout": {
        "title": "Delivery Simulator Layout",
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "dropdown__delivery--layout"
