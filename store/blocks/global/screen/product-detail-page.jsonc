    "disclosure-layout#page__dropdown--delivery": {
        "title": "Product Page Deliver Dropdown",
        "props": {
            "blockClass": [
                "page__dropdown--general",
                "page__dropdown--delivery"
            ]
        },
        "children": [
            "flex-layout.col#dropdown__delivery--layout"
        ]
    },
    "disclosure-layout#page__dropdown--details": {
        "title": "Product Page Details Dropdown",
        "props": {
            "blockClass": [
                "page__dropdown--general",
                "page__dropdown--details"
            ]
        },
        "children": [
            "flex-layout.col#dropdown__details--layout"
        ]
    },
    "disclosure-layout#page__dropdown--description": {
        "title": "Product Page Description Dropdown",
        "props": {
            "blockClass": [
                "page__dropdown--general",
                "page__dropdown--description"
            ]
        },
        "children": [
            "flex-layout.col#dropdown__description--layout"
        ]
    },
    ///////////////////////////////////////////////
    "flex-layout.row#page__order--sku": {
        "title": "Product Sku Container",
        "props": {
            "blockClass": "page__order--sku"
        },
        "children": [
            "sku-selector#order__sku"
        ]
    },
    "flex-layout.row#page__order--quantity-add": {
        "title": "Product Quantity and Add Button Container",
        "props": {
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "preserveLayoutOnMobile": true,
            "blockClass": "page__order--quantity-add"
