    ///////////////////////////////////////////////
    "flex-layout.row#product_breadcrumb--container": {
        "title": "Product Breadcrumb Container",
        "props": {
            "blockClass": "product_breadcrumb--container"
        },
        "children": [
            "flex-layout.col#product__breadcrumb"
        ]
    },
    "flex-layout.row#product_info--container": {
        "title": "Product General Information",
        "props": {
            "blockClass": "product_info--container"
        },
        "children": [
            "flex-layout.col#product__info--image",
            "flex-layout.col#product__info--data"
        ]
    },
    "flex-layout.row#product_tabs--container": {
        "title": "Product General Tabs",
        "props": {
            "blockClass": "product_tabs--container"
        },
        "children": [
            "tab-layout#product__tabs"
        ]
    },
    "modal-trigger#product__add-to-cart-info": {
        "title": "Add To Cart info Modal",
        "props": {
            "customPixelEventName": "addToCart",
            "blockClass": "product__add-to-cart-info"
        },
        "children": [
            "modal-layout#add-to-cart-info"
        ]
    },
    ///////////////////////////////////////////////
    "flex-layout.col#product__breadcrumb": {
        "title": "Product Breadcrumb",
        "props": {
            "blockClass": "product__breadcrumb"
        },
        "children": [
            "breadcrumb#breadcrumb__layout"
        ]
    },
    "flex-layout.col#product__info--image": {
        "title": "Product Image",
        "props": {
            "width": "66%",
            "blockClass": "product__info--image"
        },
        "children": [
            "product-images#info__image"
        ]
    },
    "flex-layout.col#product__info--data": {
        "title": "Product Data",
        "props": {
            "width": "34%",
            "preventVerticalStretch": true,
            "blockClass": "product__info--data"
        },
        "children": [
            "flex-layout.row#product__page--brand-container",
            "vtex.store-components:product-name#product__page--name",
            "rich-text#product__page--name-mesagge",
            "flex-layout.row#product__page--price-container",
            "flex-layout.row#product__page--order-container",
            "flex-layout.row#product__page--delivery",
            "flex-layout.row#product__page--details",
            "flex-layout.row#product__page--description"
        ]
    },
    "modal-layout#add-to-cart-info": {
        "title": "Add To Cart Info Modal Layout",
        "props": {
            "blockClass": "add-to-cart-info"
        },
        "children": [
            "add-to-cart-info"
        ]
    },
    ///////////////////////////////////////////////
    "breadcrumb#breadcrumb__layout": {
        "props": {
            "blockClass": "breadcrumb__layout"
        }
    },
    "product-images#info__image": {
        "props": {
            "zoomMode": "disabled",
            "thumbnailsOrientation": "horizontal",
            "thumbnailMaxHeight": 180,
            "blockClass": "info__image"
        }
    },
    "flex-layout.row#product__page--brand-container": {
        "title": "Product Page Brand Container",
        "props": {
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": "product__page--brand-container"
        },
        "children": [
            "image#product__page--brand-image",
            "vtex.store-components:product-brand#product__page--brand"
        ]
    },
    "vtex.store-components:product-name#product__page--name": {
        "title": "Product Name",
        "props": {
            "blockClass": "product__page-name"
        }
    },
    "rich-text#product__page--name-mesagge": {
        "title": "Product Name Message",
        "props": {
            "text": "Este articulo saldrá del almacen en un dia laborable.",
            "blockClass": "product__page--name-mesagge"
        }
    },
    "flex-layout.row#product__page--price-container": {
        "title": "Product Page Price Container",
        "props": {
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": "product__page--price-container"
        },
        "children": [
            "rich-text#page__price--message",
            "product-price#product__page--price"
        ]
    },
    "flex-layout.row#product__page--order-container": {
        "title": "Product Page Order Details Container",
        "props": {
            "preserveLayoutOnMobile": true,
            "blockClass": "product__page--order-container"
        },
        "children": [
            "flex-layout.col#page__order--layout"
        ]
    },
    "flex-layout.row#product__page--delivery": {
        "title": "Product Page Deliveries Container",
        "props": {
            "blockClass": [
                "product__page--general",
                "product__page--delivery"
            ]
        },
        "children": [
            "disclosure-layout#page__dropdown--delivery"
        ]
    },
    "flex-layout.row#product__page--details": {
        "title": "Product Page Details Container",
        "props": {
            "blockClass": [
                "product__page--general",
                "product__page--details"
            ]
        },
        "children": [
            "disclosure-layout#page__dropdown--details"
        ]
    },
    "flex-layout.row#product__page--description": {
        "title": "Product Page Description Container",
        "props": {
            "blockClass": [
                "product__page--general",
                "product__page--description"
            ]
        },
        "children": [
            "disclosure-layout#page__dropdown--description"
        ]
    },
    ///////////////////////////////////////////////
    "image#product__page--brand-image": {
        "title": "Product Brand Image",
        "props": {
            "src": "assets/img/home/icon-f1-logo.svg",
            "blockClass": "product__page--brand-image"
        }
    },
    "vtex.store-components:product-brand#product__page--brand": {
        "props": {
            "blockClass": "product__page--brand"
        }
    },
    "rich-text#page__price--message": {
        "title": "Product Price Message",
        "props": {
            "text": "Tu precio:",
            "blockClass": "page__price--message"
        }
    },
    "product-price#product__page--price": {
        "title": "Product Price Value",
        "props": {
            "blockClass": "product__page--price"
        }
    },
    "flex-layout.col#page__order--layout": {
        "title": "Product Page Order Details Layout",
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "page__order--layout"
