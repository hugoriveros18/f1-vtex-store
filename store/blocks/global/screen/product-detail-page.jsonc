    ///////////////////////////////////////////////
    "product-quantity#order__quantity": {
        "title": "Product Quantity",
        "props": {
            "selectorType": "dropdown"
        }
    },
    "add-to-cart-button#order__add-button": {
        "title": "Product Add To Cart Button",
        "props": {
            "text": "Añadir a la cesta",
            "blockClass": "order__add-button"
        }
    },
    "flex-layout.row#delivery__simulator--visible": {
        "title": "Delivery Simulator Visible",
        "props": {
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "blockClass": [
