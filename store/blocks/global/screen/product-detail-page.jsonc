        "children": [
            "disclosure-trigger#delivery__simulator",
            "disclosure-content#delivery__simulator"
        ]
    },
    "flex-layout.col#dropdown__details--layout": {
        "title": "Details Layout",
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "dropdown__details--layout"
        },
        "children": [
            "disclosure-trigger#details__section",
            "disclosure-content#details__section"
        ]
    },
    "flex-layout.col#dropdown__description--layout": {
        "title": "Details Layout",
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "dropdown__description--layout"
        },
        "children": [
            "disclosure-trigger#description__section",
            "disclosure-content#description__section"
        ]
    },
    ///////////////////////////////////////////////
    "sku-selector#order__sku": {
        "title": "Product SKU",
        "props": {
            "visibility": "always",
            "initialSelection": "complete",
            "blockClass": "order__sku"
        }
    },
    "flex-layout.col#order__quantity--layout": {
        "title": "Product Quantity Layout",
        "props": {
            "width": "20%",
            "blockClass": "order__quantity--layout"
        },
        "children": [
            "product-quantity#order__quantity"
        ]
    },
    "flex-layout.col#order__add--button-layout": {
        "title": "Product Add To Cart Button Layout",
        "props": {
            "width": "grow",
            "blockClass": "order__add--button-layout"
